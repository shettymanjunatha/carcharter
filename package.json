{
  "name": "carcharter",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "explorer": "ng build --source-map  && source-map-explorer dist/carcharter/**/*.js",
    "json-server": "json-server -p 3002 server/db.json",
    "server": "node server/index.js",
    "dev-build": "ng build --configuration=dev && http-server dist/carcharter -p 8000 -o",
    "prod-build": "ng build && http-server dist/carcharter -p 9000 -o"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.1.0",
    "@angular/common": "^14.1.0",
    "@angular/compiler": "^14.1.0",
    "@angular/core": "^14.1.0",
    "@angular/forms": "^14.1.0",
    "@angular/platform-browser": "^14.1.0",
    "@angular/platform-browser-dynamic": "^14.1.0",
    "@angular/router": "^14.1.0",
    "@ngrx/effects": "^14.0.2",
    "@ngrx/entity": "^14.0.2",
    "@ngrx/store": "^14.0.2",
    "@ngrx/store-devtools": "^14.0.2",
    "autoprefixer": "10.4.5",
    "bootstrap": "^5.2.0",
    "json-server": "^0.17.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.0",
    "@angular/cli": "~14.1.0",
    "@angular/compiler-cli": "^14.1.0",
    "@ngrx/schematics": "^14.0.2",
    "@types/jest": "^28.1.6",
    "@types/uuid": "^8.3.4",
    "jest": "^28.1.3",
    "jest-preset-angular": "^12.2.0",
    "ts-jest": "^28.0.7",
    "typescript": "~4.7.2"
  },
  "overrides": {
    "autoprefixer": "10.4.5"
  }
}
